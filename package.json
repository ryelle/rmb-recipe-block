{
  "name": "rmb-recipe-block",
  "title": "Recipe Block",
  "version": "1.0.0",
  "homepage": "https://ryelle.codes/",
  "repository": {
    "type": "git",
    "url": "https://github.com:ryelle/rmb-recipe-block.git"
  },
  "license": "GPL-2.0+",
  "main": "build/recipe-block.js",
  "scripts": {
    "build": "cross-env BABEL_ENV=default NODE_ENV=production webpack",
    "start": "cross-env BABEL_ENV=default webpack --watch",
    "ci": "npm run lint",
    "lint": "concurrently \"npm run lint-js\" \"npm run lint-css\" \"npm run lint-php\"",
    "lint-js": "eslint . --ext=js,jsx",
    "lint-js:fix": "eslint . --ext=js,jsx --fix",
    "lint-css": "stylelint '**/*.scss'",
    "lint-css:fix": "stylelint '**/*.scss' --fix",
    "lint-php": "composer run lint"
  },
  "devDependencies": {
    "@babel/core": "7.5.4",
    "@wordpress/babel-preset-default": "4.3.0",
    "@wordpress/eslint-plugin": "2.3.0",
    "autoprefixer": "9.6.1",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "1.0.1",
    "concurrently": "4.1.1",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "cssnano": "4.1.10",
    "eslint": "6.0.1",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "10.1.0",
    "stylelint-config-wordpress": "14.0.0",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.4"
  },
  "engines": {
    "node": "10.16.0",
    "npm": "6.10.0"
  },
  "files": [
    "build/*",
    "README.md",
    "rmb-recipe-block.php"
  ]
}

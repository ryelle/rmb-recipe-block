{
  "name": "rmb-recipe-block",
  "title": "Recipe Block",
  "version": "1.1.0",
  "author": "Kelly Dwan & Mel Choyce",
  "license": "GPL-2.0-or-later",
  "homepage": "https://ryelle.codes/",
  "repository": {
    "type": "git",
    "url": "https://github.com:ryelle/rmb-recipe-block.git"
  },
  "engines": {
    "node": "12.14.1",
    "npm": "6.14.5"
  },
  "files": [
    "build/*",
    "README.md",
    "rmb-recipe-block.php"
  ],
  "main": "build/recipe-block.js",
  "devDependencies": {
    "@wordpress/browserslist-config": "2.6.0",
    "@wordpress/eslint-plugin": "3.4.1",
    "@wordpress/npm-package-json-lint-config": "2.2.0",
    "@wordpress/scripts": "7.0.1",
    "autoprefixer": "9.8.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.2.0",
    "css-loader": "3.5.3",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "postcss-loader": "3.0.0",
    "sass-loader": "8.0.2",
    "stylelint": "13.6.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "format:js": "wp-scripts format-js",
    "lint:css": "wp-scripts lint-style assets/css/*",
    "lint:css:fix": "wp-scripts lint-style assets/css/* --fix",
    "lint:js": "wp-scripts lint-js assets/js/*",
    "lint:js:fix": "wp-scripts lint-js assets/js/* --fix",
    "lint:php": "composer run lint",
    "lint:php:fix": "composer run format",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "lint": "concurrently \"npm run lint:js\" \"npm run lint:css\" \"npm run lint:php\"",
    "ci": "npm run lint"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "eslintIgnore": [
    "*.min.js"
  ],
  "npmpackagejsonlint": {
    "extends": "@wordpress/npm-package-json-lint-config",
    "rules": {
      "require-bugs": "off",
      "require-description": "off",
      "require-keywords": "off"
    }
  }
}
